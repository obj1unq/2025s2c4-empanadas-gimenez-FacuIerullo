import empleados.*

test "punto1: Planteo inicial" {

  baigorria.ventaDeEmpanadas(500)
  galvan.cambioDeSueldo(20000)
  baigorria.ventaDeEmpanadas(300)
  assert.equals(12000, baigorria.sueldoActual())
  assert.equals(20000, galvan.sueldoActual())
  
  gimenez.pagarSueldo(baigorria)
  gimenez.pagarSueldo(galvan)
  assert.equals(268000, gimenez.fondoActual())
  
}

